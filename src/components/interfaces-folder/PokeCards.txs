interface pokemons {
  name: string;
  imageIndex: number;
  image: string;
}

interface url {
  url: string;
}

interface imageURL {
  imageUrl: string;
}

interface addSelectedPokemons {
  information: pokemons;
  url: url;
  image: imageURL;
}

interface Props {
  name: string;
  imageIndex: string;
  setShow: boolean | unknown;
  addSelectedPokemon: addSelectedPokemons | unknown;
  singleViewState: boolean;
  url: string;
}